@using WebApplication2.Models
@model List<Employee>

<h3>Welcome to Employee page</h3>
<hr/>

<a href="/Employee/Create">Create New </a>
<table border="4" width="800">
<tr>
        <th>Employee Id</th>
        <th>Employee Name</th>
        <th>Employee Job</th>
        <th>Employee Salary</th>
        <th>Employee Deptno</th>
        <th></th>
</tr>
 @foreach(Employee item in Model)
    {
         <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.Job</td>
            <td>@item.Salary</td>  
            <td>@item.Deptno</td>
            <td>
                <a href="/Employee/Details/@item.EmpId">Details</a> |
                <a href="/Employee/Edit/@item.EmpId">Edit</a> |
                <a href="/Employee/Delete/@item.EmpId">Delete</a>  
            </td>
         </tr>
    }
</table>